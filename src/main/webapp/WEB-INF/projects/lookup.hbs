{{>layout/header}}

  {{>layout/project_side}}

      <div class="col-lg-9">
        <div id="blank"></div>
        <div class="row">

            <div class="col-lg-9 col-md-7 mb-5">
            <form class="project-container">
            <div class="form-group">
                <label for="title">제목</label>
                <input type="text" class="form-control" id="title" value="{{project.title}}" readonly>
            </div>
            <div class="form-group">
                <label for="title">수행기간</label>
                <input type="text" class="form-control-inline" id="start-date" value="{{project.startDate}}" readonly>
                ~ &nbsp;
                <input type="text" class="form-control-inline" id="end-date" value="{{project.endDate}}" readonly>
            </div>
            <div class="form-group" id="member-names">
                <label for="author">참여멤버</label>
            </div>
            <div class="form-group">
                <label for="author">종류</label>
                <input type="text" class="form-control-inline" id="project-type" value="{{project.projectType}}" readonly>
            </div>
            <div class="form-group">
                <label for="situation">문제상황</label>
                <textarea class="form-control" id="situation" readonly>{{project.situation}}</textarea>
            </div>
            <div class="form-group">
                <label for="author">내용</label>
                <textarea class="form-control" id="scenario" readonly>{{project.content}}</textarea>
            </div>
            <div class="form-group">
                <label for="content">테스트 시나리오</label>
                <textarea class="form-control" id="content" readonly>{{project.testScenario}}</textarea>
            </div>
            </form>
                <a href="/diary/projects/cards" role="button" class="btn btn-secondary">취소</a>
                <a href="/diary/projects/modification/{{project.id}}" role="button" class="btn btn-secondary">수정</a>
                <button type="button" class="btn btn-secondary" id="project-delete" value="{{project.id}}">삭제</button>
            </div>
        </div>
        <!-- /.row -->

      </div>
      <!-- /.col-lg-9 -->

    </div>
    <!-- /.row -->

  </div>
  <!-- /.container -->
  <script type="text/javascript" src="http://code.jquery.com/jquery-3.5.1.min.js"></script>
  <script src="js/lookup.js?ver=1"></script>

{{>layout/footer}}